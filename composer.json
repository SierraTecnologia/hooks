{
    "name": "sierratecnologia/hooks",
    "description": "A Laravel Hook system",
    "keywords": ["laravel", "hooks"],
    "license": "MIT",
    "homepage": "https://sierratecnologia.io/",
    "support": {
        "issues": "https://github.com/sierratecnologia/hooks/issues",
        "source": "https://github.com/sierratecnologia/hooks"
    },
    "authors": [
        {
            "name": "Mark Topper",
            "email": "mark@ulties.com"
        }
    ],
    "require": {
        "composer/composer": ">=1.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Hooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hooks\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}

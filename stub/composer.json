{
    "name": "kebab-case",
    "description": "This is my first hook.",
    "require": {
        "larapack/hooks": "^1.0.5"
    },
    "autoload": {
        "psr-4": {
            "StudlyCase\\": "src/"
        }
    },
    "extra": {
        "hook": {
            "providers": [
                "StudlyCase\\StudlyCaseServiceProvider"
            ],
            "aliases": {
                "StudlyCase": "StudlyCase\\StudlyCaseFacade"
            },
            "migrations": [
                "resources/database/migrations"
            ],
            "seeders": [
                "resources/database/seeds/DatabaseSeeder.php"
            ],
            "unseeders": [
                "resources/database/unseeds/DatabaseUnseeder.php"
            ],
            "assets": {
                "resources/assets": "public/vendor/kebab-case"
            }
        }
    }
}